{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Kedi",
  "scopeName": "source.kedi",
  "patterns": [
    {
      "include": "#block-comments"
    },
    {
      "include": "#standalone-comment"
    },
    {
      "include": "#type-definition"
    },
    {
      "include": "#test-definition"
    },
    {
      "include": "#eval-definition"
    },
    {
      "include": "#procedure-definition"
    },
    {
      "include": "#return-python-block"
    },
    {
      "include": "#assignment-python-block"
    },
    {
      "include": "#return-statement"
    },
    {
      "include": "#assignment"
    },
    {
      "include": "#python-block"
    },
    {
      "include": "#template-line"
    }
  ],
  "repository": {
    "block-comments": {
      "patterns": [
        {
          "begin": "^\\s*(###)\\s*$",
          "end": "^\\s*(###)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "comment.block.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "comment.block.kedi"
            }
          },
          "contentName": "comment.block.kedi"
        }
      ]
    },
    "standalone-comment": {
      "patterns": [
        {
          "match": "^\\s*(?<!#)(#)(?!#).*",
          "name": "comment.line.number-sign.kedi",
          "captures": {
            "1": {
              "name": "punctuation.definition.comment.kedi"
            }
          }
        }
      ]
    },
    "type-definition": {
      "patterns": [
        {
          "begin": "^\\s*(~)([A-Za-z_][A-Za-z0-9_]*)",
          "end": "$",
          "beginCaptures": {
            "1": { "name": "keyword.operator.tilde.kedi" },
            "2": { "name": "entity.name.type.kedi" }
          },
          "patterns": [
            {
              "begin": "\\(",
              "end": "\\)",
              "beginCaptures": {
                "0": { "name": "punctuation.section.parameters.begin.kedi" }
              },
              "endCaptures": {
                "0": { "name": "punctuation.section.parameters.end.kedi" }
              },
              "patterns": [
                { "include": "#type-fields" },
                { "include": "#inline-comment" }
              ]
            },
            { "include": "#inline-comment" }
          ],
          "name": "meta.type.definition.kedi"
        }
      ]
    },
    "type-fields": {
      "patterns": [
        {
          "begin": "([A-Za-z_][A-Za-z0-9_]*)\\s*(:)\\s*(`)(?![^`]*``)",
          "end": "(`)",
          "beginCaptures": {
            "1": {
              "name": "variable.other.field.kedi"
            },
            "2": {
              "name": "punctuation.separator.kedi"
            },
            "3": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "contentName": "storage.type.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        },
        {
          "match": "([A-Za-z_][A-Za-z0-9_]*)\\s*(:)\\s*([A-Za-z_][A-Za-z0-9_\\[\\]]*)",
          "captures": {
            "1": {
              "name": "variable.other.field.kedi"
            },
            "2": {
              "name": "punctuation.separator.kedi"
            },
            "3": {
              "name": "storage.type.kedi"
            }
          }
        },
        {
          "match": "([A-Za-z_][A-Za-z0-9_]*)",
          "name": "variable.other.field.kedi"
        },
        {
          "match": ",",
          "name": "punctuation.separator.parameters.kedi"
        },
        {
          "include": "#inline-comment"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "match": "(?<!#)(#)(?!#).*$",
          "name": "comment.line.number-sign.kedi",
          "captures": {
            "1": {
              "name": "punctuation.definition.comment.kedi"
            }
          }
        }
      ]
    },
    "test-definition": {
      "patterns": [
        {
          "begin": "^\\s*(@)(test)(:)\\s*([A-Za-z_][A-Za-z0-9_]*)\\s*(:)",
          "end": "(?=^\\s*[@~]|^\\s*\\[|^\\s*=(?!=)|^\\s*```\\s*$|^(?!\\s))",
          "beginCaptures": {
            "1": { "name": "keyword.operator.kedi" },
            "2": { "name": "storage.type.function.kedi" },
            "3": { "name": "punctuation.separator.kedi" },
            "4": { "name": "entity.name.function.kedi" },
            "5": { "name": "punctuation.separator.kedi" }
          },
          "name": "meta.test.definition.kedi",
          "patterns": [
            { "include": "#case-block" },
            { "include": "#inline-comment" }
          ]
        }
      ]
    },
    "eval-definition": {
      "patterns": [
        {
          "begin": "^\\s*(@)(eval)(:)\\s*([A-Za-z_][A-Za-z0-9_]*)\\s*(:)",
          "end": "(?=^\\s*[@~]|^\\s*\\[|^\\s*=(?!=)|^\\s*```\\s*$|^(?!\\s))",
          "beginCaptures": {
            "1": { "name": "keyword.operator.kedi" },
            "2": { "name": "storage.type.function.kedi" },
            "3": { "name": "punctuation.separator.kedi" },
            "4": { "name": "entity.name.function.kedi" },
            "5": { "name": "punctuation.separator.kedi" }
          },
          "name": "meta.eval.definition.kedi",
          "patterns": [
            { "include": "#metric-block" },
            { "include": "#inline-comment" }
          ]
        }
      ]
    },
    "case-block": {
      "patterns": [
        {
          "begin": "^\\s*(>)\\s*(case)\\s*(:)\\s*([A-Za-z_][A-Za-z0-9_]*)\\s*(:)?",
          "end": "(?=^\\s*>\\s*(?:case|metric)\\s*:|^\\s*[@~]|^\\s*\\[|^\\s*=(?!=)|^\\s*```\\s*$|^(?=\\S))",
          "beginCaptures": {
            "1": { "name": "keyword.operator.kedi" },
            "2": { "name": "storage.type.function.kedi" },
            "3": { "name": "punctuation.separator.kedi" },
            "4": { "name": "entity.name.label.kedi" }
          },
          "name": "meta.case.block.kedi",
          "patterns": [
            { "include": "#python-block" },
            { "include": "#assignment-python-block" },
            { "include": "#return-python-block" },
            { "include": "#inline-python" },
            { "include": "#assignment" },
            { "include": "#return-statement" },
            { "include": "#standalone-comment" },
            { "include": "#template-line" }
          ]
        }
      ]
    },
    "metric-block": {
      "patterns": [
        {
          "begin": "^\\s*(>)\\s*(metric)\\s*(:)\\s*([A-Za-z_][A-Za-z0-9_]*)\\s*(:)?",
          "end": "(?=^\\s*>\\s*(?:case|metric)\\s*:|^\\s*[@~]|^\\s*\\[|^\\s*=(?!=)|^\\s*```\\s*$|^(?=\\S))",
          "beginCaptures": {
            "1": { "name": "keyword.operator.kedi" },
            "2": { "name": "storage.type.function.kedi" },
            "3": { "name": "punctuation.separator.kedi" },
            "4": { "name": "entity.name.label.kedi" }
          },
          "name": "meta.metric.block.kedi",
          "patterns": [
            { "include": "#python-block" },
            { "include": "#assignment-python-block" },
            { "include": "#return-python-block" },
            { "include": "#inline-python" },
            { "include": "#assignment" },
            { "include": "#return-statement" },
            { "include": "#standalone-comment" },
            { "include": "#template-line" }
          ]
        }
      ]
    },
    "procedure-definition": {
      "patterns": [
        {
          "begin": "^\\s*(@)([A-Za-z_][A-Za-z0-9_]*)",
          "end": "$",
          "beginCaptures": {
            "1": { "name": "keyword.operator.kedi" },
            "2": { "name": "entity.name.function.kedi" }
          },
          "name": "meta.procedure.definition.kedi",
          "patterns": [
            {
              "begin": "\\(",
              "end": "\\)",
              "beginCaptures": {
                "0": { "name": "punctuation.section.parameters.begin.kedi" }
              },
              "endCaptures": {
                "0": { "name": "punctuation.section.parameters.end.kedi" }
              },
              "patterns": [
                { "include": "#parameters" }
              ]
            },
            {
              "begin": "\\s*(->)\\s*(`)(?![^`]*``)",
              "end": "(`)",
              "beginCaptures": {
                "1": { "name": "keyword.operator.arrow.kedi" },
                "2": { "name": "punctuation.definition.code.kedi" }
              },
              "endCaptures": {
                "1": { "name": "punctuation.definition.code.kedi" }
              },
              "contentName": "storage.type.kedi",
              "patterns": [
                {
                  "include": "source.python"
                }
              ]
            },
            {
              "match": "\\s*(->)\\s*([A-Za-z_][A-Za-z0-9_\\[\\]]*)?",
              "captures": {
                "1": { "name": "keyword.operator.arrow.kedi" },
                "2": { "name": "storage.type.kedi" }
              }
            },
            {
              "match": "\\s*(:)",
              "captures": {
                "1": { "name": "punctuation.section.procedure.kedi" }
              }
            },
            { "include": "#inline-comment" }
          ]
        }
      ]
    },
    "parameters": {
      "patterns": [
        {
          "begin": "([A-Za-z_][A-Za-z0-9_]*)\\s*(:)\\s*(`)(?![^`]*``)",
          "end": "(`)",
          "beginCaptures": {
            "1": {
              "name": "variable.parameter.kedi"
            },
            "2": {
              "name": "punctuation.separator.kedi"
            },
            "3": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "contentName": "storage.type.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        },
        {
          "match": "([A-Za-z_][A-Za-z0-9_]*)\\s*(:)\\s*([A-Za-z_][A-Za-z0-9_\\[\\]]*)",
          "captures": {
            "1": {
              "name": "variable.parameter.kedi"
            },
            "2": {
              "name": "punctuation.separator.kedi"
            },
            "3": {
              "name": "storage.type.kedi"
            }
          }
        },
        {
          "match": "([A-Za-z_][A-Za-z0-9_]*)",
          "name": "variable.parameter.kedi"
        },
        {
          "match": ",",
          "name": "punctuation.separator.parameters.kedi"
        }
      ]
    },
    "return-python-block": {
      "patterns": [
        {
          "begin": "^\\s*(=)\\s*(```)\\s*$",
          "end": "^\\s*(```)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.return.kedi"
            },
            "2": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "contentName": "source.python.embedded.block.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        }
      ]
    },
    "assignment-python-block": {
      "patterns": [
        {
          "begin": "^\\s*(\\[)([A-Za-z_][A-Za-z0-9_]*)\\s*(:)\\s*(`)(?=[^`]*```)",
          "end": "^\\s*(```)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.variable.begin.kedi"
            },
            "2": {
              "name": "variable.other.assignment.kedi"
            },
            "3": {
              "name": "punctuation.separator.kedi"
            },
            "4": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "patterns": [
            {
              "begin": "^(```)\\s*$",
              "end": "^\\s*(```)\\s*$",
              "beginCaptures": {
                "1": {
                  "name": "punctuation.definition.code.kedi"
                }
              },
              "endCaptures": {
                "1": {
                  "name": "punctuation.definition.code.kedi"
                }
              },
              "contentName": "source.python.embedded.block.kedi",
              "patterns": [
                {
                  "include": "source.python"
                }
              ]
            }
          ]
        },
        {
          "begin": "^\\s*(\\[)([A-Za-z_][A-Za-z0-9_]*)\\s*(:)?\\s*([A-Za-z_][A-Za-z0-9_\\[\\]]*)?\\s*(\\])\\s*(=)\\s*(```)\\s*$",
          "end": "^\\s*(```)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.variable.begin.kedi"
            },
            "2": {
              "name": "variable.other.assignment.kedi"
            },
            "3": {
              "name": "punctuation.separator.kedi"
            },
            "4": {
              "name": "storage.type.kedi"
            },
            "5": {
              "name": "punctuation.definition.variable.end.kedi"
            },
            "6": {
              "name": "keyword.operator.assignment.kedi"
            },
            "7": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "contentName": "source.python.embedded.block.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        }
      ]
    },
    "return-statement": {
      "patterns": [
        {
          "begin": "^\\s*(=)(?!=)",
          "end": "$",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.return.kedi"
            }
          },
          "patterns": [
            {
              "include": "#expression-contents-no-output"
            },
            {
              "include": "#inline-comment"
            }
          ]
        }
      ]
    },
    "assignment": {
      "patterns": [
        {
          "begin": "^\\s*(\\[)([A-Za-z_][A-Za-z0-9_]*)\\s*(:)\\s*(`)(?![^`]*``)",
          "end": "(`)\\s*(\\])\\s*(=)(?!=)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.variable.kedi"
            },
            "2": {
              "name": "variable.other.assignment.kedi"
            },
            "3": {
              "name": "punctuation.separator.kedi"
            },
            "4": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            },
            "2": {
              "name": "punctuation.definition.variable.kedi"
            },
            "3": {
              "name": "keyword.operator.assignment.kedi"
            }
          },
          "contentName": "storage.type.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        },
        {
          "begin": "^\\s*(\\[)([A-Za-z_][A-Za-z0-9_]*)\\s*(:)?\\s*([A-Za-z_][A-Za-z0-9_\\[\\]]*)?\\s*(\\])\\s*(=)(?!=)",
          "end": "$",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.variable.kedi"
            },
            "2": {
              "name": "variable.other.assignment.kedi"
            },
            "3": {
              "name": "punctuation.separator.kedi"
            },
            "4": {
              "name": "storage.type.kedi"
            },
            "5": {
              "name": "punctuation.definition.variable.kedi"
            },
            "6": {
              "name": "keyword.operator.assignment.kedi"
            }
          },
          "patterns": [
            {
              "include": "#expression-contents-no-output"
            },
            {
              "include": "#inline-comment"
            }
          ]
        }
      ]
    },
    "python-block": {
      "patterns": [
        {
          "begin": "^\\s*(```)\\s*$",
          "end": "^\\s*(```)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "contentName": "source.python.embedded.block.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        }
      ]
    },
    "template-line": {
      "patterns": [
        {
          "match": "^(?!\\s*[@#=]|\\s*\\[[^]]*\\]\\s*=|\\s*```|\\s*> case:|\\s*> metric:|\\s*$)(.+)$",
          "captures": {
            "1": {
              "patterns": [
                { "include": "#inline-comment" },
                { "include": "#template-contents" }
              ]
            }
          }
        }
      ]
    },
    "expression-contents": {
      "patterns": [
        { "include": "#escaped-chars" },
        {
          "include": "#inline-python"
        },
        {
          "include": "#substitution"
        },
        {
          "include": "#output"
        },
        {
          "include": "#strings"
        },
        {
          "include": "#inline-comment"
        }
      ]
    },
    "expression-contents-no-output": {
      "patterns": [
        { "include": "#escaped-chars" },
        {
          "include": "#inline-python"
        },
        {
          "include": "#substitution"
        },
        {
          "include": "#output"
        },
        {
          "include": "#strings"
        },
        {
          "include": "#inline-comment"
        },
        {
          "match": "[^#<`\\[\\]@=,>\\\\\\s]+",
          "name": "string.unquoted.kedi"
        }
      ]
    },
    "template-contents": {
      "patterns": [
        { "include": "#escaped-chars" },
        {
          "include": "#inline-python"
        },
        {
          "include": "#substitution"
        },
        {
          "include": "#output"
        },
        {
          "include": "#strings"
        },
        {
          "include": "#inline-comment"
        },
        {
          "match": "[^#<`\\[\\]@=,>\\\\\\s]+",
          "name": "string.unquoted.kedi"
        }
      ]
    },
    "inline-python": {
      "patterns": [
        {
          "begin": "(<)(`)",
          "end": "(`)(>)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.variable.begin.kedi"
            },
            "2": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            },
            "2": {
              "name": "punctuation.definition.variable.end.kedi"
            }
          },
          "contentName": "source.python.embedded.inline.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        },
        {
          "begin": "(`)",
          "end": "(`)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "name": "meta.embedded.inline.python.kedi",
          "contentName": "source.python.embedded.inline.kedi",
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        }
      ]
    },
    "substitution": {
      "patterns": [
        {
          "begin": "(<)(?!<)([A-Za-z_][A-Za-z0-9_]*)\\s*(\\()",
          "end": "(\\))(>)(?!>)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.variable.begin.kedi"
            },
            "2": {
              "name": "entity.name.function.call.kedi"
            },
            "3": {
              "name": "punctuation.section.arguments.begin.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.section.arguments.end.kedi"
            },
            "2": {
              "name": "punctuation.definition.variable.end.kedi"
            }
          },
          "patterns": [
            {
              "include": "#call-arguments"
            }
          ]
        },
        {
          "match": "(<)(?!<)([A-Za-z_][A-Za-z0-9_]*)(>)(?!>)",
          "captures": {
            "1": {
              "name": "punctuation.definition.variable.begin.kedi"
            },
            "2": {
              "name": "variable.other.readwrite.kedi"
            },
            "3": {
              "name": "punctuation.definition.variable.end.kedi"
            }
          }
        }
      ]
    },
    "call-arguments": {
      "patterns": [
        {
          "begin": "`",
          "end": "`",
          "name": "meta.embedded.argument.python",
          "contentName": "source.python.embedded.arg.kedi",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "patterns": [
            { "include": "source.python" }
          ]
        },
        { "include": "#escaped-chars" },
        { "include": "#inline-python" },
        { "include": "#substitution" },
        { "include": "#strings" },
        { "match": ",", "name": "punctuation.separator.arguments.kedi" },
        {
          "match": "[^,<>`\\n\\r\\[\\]\\)]+",
          "name": "string.unquoted.argument.kedi"
        }
      ]
    },
    "output": {
      "patterns": [
        {
          "begin": "(\\[)(?!\\[)([A-Za-z_][A-Za-z0-9_]*)\\s*(:)\\s*(`)(?![^`]*``)",
          "end": "(`)(\\])(?!\\])",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.variable.begin.kedi"
            },
            "2": {
              "name": "variable.other.output.kedi"
            },
            "3": {
              "name": "punctuation.separator.kedi"
            },
            "4": {
              "name": "punctuation.definition.code.kedi"
            }
          },
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.code.kedi"
            },
            "2": {
              "name": "punctuation.definition.variable.end.kedi"
            }
          },
